{
  "name": "quiz-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--inspect && next dev",
    "build": "npm run migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "npm run migrate:test && dotenv -e .env.test -- jest",
    "migrate": "prisma generate && prisma db push --accept-data-loss",
    "migrate:test": "dotenv -e .env.test -- prisma migrate reset -f",
    "inspect": "npx eslint --inspect-config",
    "hash:password": "node hashPassword.mjs",
    "generate:paths": "node generateRoutes.mjs ./src/app ./src/lib/path.ts one false",
    "generate:diff-prompt": "node generatePromptFromGitDiff.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "jszip": "^3.10.1",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "pdf-parse": "^1.1.1",
    "quiz-app": "file:",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "swr": "^2.2.5",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.14.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  }
}
