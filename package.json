{
  "name": "quiz-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--inspect && next dev",
    "build": "npm run migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run migrate:test && dotenv -e .env.test -- jest",
    "migrate": "prisma generate && prisma db push --accept-data-loss",
    "migrate:test": "dotenv -e .env.test -- prisma migrate reset -f"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.1",
    "jszip": "^3.10.1",
    "next": "14.2.15",
    "next-auth": "^4.24.8",
    "pdf-parse": "^1.1.1",
    "quiz-app": "file:",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "swr": "^2.2.5",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.13",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^22",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
