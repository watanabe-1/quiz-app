{
  "name": "quiz-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate:paths && set NODE_OPTIONS=--inspect && next dev",
    "build": "npm run generate:paths && npm run migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "npm run migrate:test && dotenv -e .env.test -- jest",
    "migrate": "prisma generate && prisma db push --accept-data-loss",
    "migrate:test": "dotenv -e .env.test -- prisma migrate reset -f",
    "inspect": "npx eslint --inspect-config",
    "hash:password": "node hashPassword.mjs",
    "generate:paths": "node generateRoutes.mjs ./src/app ./src/lib/path.ts one false",
    "generate:diff-prompt": "node generatePromptFromGitDiff.mjs"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@prisma/client": "^6.1.0",
    "bcryptjs": "^2.4.3",
    "jszip": "^3.10.1",
    "next": "15.1.3",
    "next-auth": "^5.0.0-beta.25",
    "pdf-parse": "^1.1.1",
    "quiz-app": "file:",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "swr": "^2.3.0",
    "tailwind-variants": "^0.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.1.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "whatwg-fetch": "^3.6.20"
  }
}
